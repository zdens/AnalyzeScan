			<xsl:call-template name="altova:html-table-cff-comment-calc-col-count-row">
				<xsl:with-param name="row" select="$tbody/tr[1]"/>
				<xsl:with-param name="rowinfo" select="''"/>
			</xsl:call-template>
		</xsl:variable>
		<xsl:variable name="new-col-count-max">
			<xsl:choose>
