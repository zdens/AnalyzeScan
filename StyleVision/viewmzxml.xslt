			<xsl:value-of select="concat('background-color:',$bgcolor,';')"/>
		</xsl:if>
		<xsl:variable name="effective-border">
			<xsl:variable name="border" select="normalize-space(string($table/@border))"/>
			<xsl:choose>
				<xsl:when test="string-length($border) > 0">
